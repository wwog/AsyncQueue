{
  "$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
  "files": {
    "include": [
      "src/**/*.ts"
    ]
  },
  "extends": [],
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "complexity": {
        "noForEach": "off"
      }
    }
  },
  "formatter": {
    "enabled": true
  }
}